#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"enter number of elements";
    cin>>n;
    int ar[n];
    int sum = 0;
    int avg = 0;
    for (int i=0; i<n; i++) {
        cin>>ar[i];
        sum = sum + ar[i];
    }
    avg = sum/n;
    cout<<"average is : "<<avg;

    return 0;
}
